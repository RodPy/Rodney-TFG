	langid = {english},
	keywords = {Amperometric {pH}-sensing, Biosensor, Oxalacetate, Penicillin, Urea},
	file = {ScienceDirect Snapshot:C\:\\Users\\rodne\\Zotero\\storage\\ND72ZQLD\\S0003267000008692.html:text/html},
