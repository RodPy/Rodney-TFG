	urldate = {2021-10-09},
	date = {2016},
	langid = {english},
	doi = {10.1007/698_2015_391},
	keywords = {Chironomidae, Crustaceans, {ECELS}, {EQAT}, Habitat condition, Insects, {QAELS}, Shallow lakes, Transitional waters, Wetlands - {WFD}},
